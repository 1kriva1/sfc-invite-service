{
  "$schema": "https://json.schemastore.org/launchsettings.json",
  "profiles": {
    "SFC.Invite": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_URLS": "https://+:7666;http://+:7665",
        "ASPNETCORE_HTTPS_PORT": "7666",
        "ConnectionStrings__Database": "Server=127.0.0.1,1433;Database=Invite;User Id=sfc;Password=Test1234!;TrustServerCertificate=True;",
        "ConnectionStrings__Hangfire": "Server=127.0.0.1,1433;Database=Hangfire;User Id=sfc;Password=Test1234!;TrustServerCertificate=True;",
        "ConnectionStrings__RabbitMq": "rabbitmq://localhost:5672",
        "ConnectionStrings__Redis": "localhost:6379",
        "Endpoints__Identity__Uri": "https://localhost:7266",
        "Endpoints__Player__Uri": "https://localhost:7366",
        "Endpoints__Team__Uri": "https://localhost:7566",
        "Kestrel__Certificates__Default__Path": "C:/SFC/Configurations/certificates/sfc-invite/sfc-invite.pfx",
        "Kestrel__Certificates__Default__Password": "Test1234!"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "https://localhost:7666;http://localhost:7665",
      "httpPort": 7665,
      "useSSL": true,
      "sslPort": 7666,
      "nativeDebugging": true
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_URLS": "https://+:7666;http://+:7665",
        "ASPNETCORE_HTTPS_PORT": "7666"
      },
      "mycontainer": "sfc-invite",
      "DockerfileRunArguments": "--network sfc-network --volume C:/SFC/Configurations/certificates/sfc-invite/sfc-invite.pfx:/https/sfc-invite.pfx",
      "httpPort": 7665,
      "useSSL": true,
      "sslPort": 7666,
      "containerName": "sfc-invite"
    }
  }
}